<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:orientation="vertical"
    android:background="@drawable/backgroundputih"
    tools:context="net.fanidyacinthya.penulisanilmiah.daftarkampus">

  <ImageView
      android:layout_width="180dp"
      android:layout_height="150dp"
      android:src="@drawable/depok"
      android:layout_alignParentBottom="true"
      android:layout_centerHorizontal="true"
      android:layout_marginBottom="400dp"
      android:onClick="depok"/>

  <ImageView
      android:layout_width="190dp"
      android:layout_height="150dp"
      android:src="@drawable/kalimalang"
      android:layout_alignParentBottom="true"
      android:layout_centerHorizontal="true"
      android:layout_marginBottom="310dp"
      android:onClick="kalimalang"
      android:id="@+id/imageView13" />

  <ImageView
      android:layout_width="180dp"
      android:layout_height="150dp"
      android:src="@drawable/karawaci"
      android:layout_alignParentBottom="true"
      android:layout_centerHorizontal="true"
      android:layout_marginBottom="255dp"
      android:onClick="karawaci"/>

  <ImageView
      android:layout_width="180dp"
      android:layout_height="150dp"
      android:src="@drawable/cengkareng"
      android:layout_alignParentBottom="true"
      android:layout_centerHorizontal="true"
      android:layout_marginBottom="200dp"
      android:onClick="cengkareng"/>

  <ImageView
      android:layout_width="180dp"
      android:layout_height="150dp"
      android:src="@drawable/kenari"
      android:layout_alignParentBottom="true"
      android:layout_centerHorizontal="true"
      android:layout_marginBottom="145dp"
      android:id="@+id/imageView12"
      android:onClick="kenari"/>

  <ImageView
      android:layout_width="180dp"
      android:layout_height="150dp"
      android:src="@drawable/salemba"
      android:layout_alignParentBottom="true"
      android:layout_centerHorizontal="true"
      android:layout_marginBottom="90dp"
      android:id="@+id/imageView11"
      android:onClick="salemba"/>

  <ImageView
      android:layout_width="190dp"
      android:layout_height="150dp"
      android:src="@drawable/simatupang"
      android:layout_alignParentBottom="true"
      android:layout_centerHorizontal="true"
      android:layout_marginBottom="35dp"
      android:id="@+id/imageView10"
      android:onClick="simatupang"/>

  <ImageView
      android:layout_width="190dp"
      android:layout_height="150dp"
      android:src="@drawable/logokembali"
      android:onClick="kembali"
      android:layout_marginTop="122dp"
      android:layout_alignTop="@+id/imageView10"
      android:layout_alignLeft="@+id/imageView10"
      android:layout_alignStart="@+id/imageView10" />


</RelativeLayout>
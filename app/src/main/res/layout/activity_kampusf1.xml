<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:orientation="vertical"
    android:background="@drawable/backgroundputih"
    tools:context="net.fanidyacinthya.penulisanilmiah.kampusf1">

    <ImageView
        android:layout_width="300dp"
        android:layout_height="250dp"
        android:src="@drawable/logof1"
        android:id="@+id/imageView56"
        android:layout_marginBottom="392dp"
        android:layout_above="@+id/imageView60"
        android:layout_centerHorizontal="true" />

    <ImageView
        android:layout_width="400dp"
        android:layout_height="250dp"
        android:src="@drawable/kampus_f1a"
        android:layout_marginTop="90dp"
        android:layout_alignTop="@+id/imageView56"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:id="@+id/imageView58" />

    <ImageView
        android:layout_width="190dp"
        android:layout_height="70dp"
        android:src="@drawable/iconinformasi"
        android:id="@+id/imageView59"
        android:layout_below="@+id/imageView58"
        android:layout_centerHorizontal="true"
        android:onClick="informasi" />

    <ImageView
        android:layout_width="190dp"
        android:layout_height="60dp"
        android:src="@drawable/iconlokasi"
        android:layout_below="@+id/imageView59"
        android:layout_alignLeft="@+id/imageView59"
        android:layout_alignStart="@+id/imageView59"
        android:onClick="lokasi" />


    <ImageView
        android:layout_width="155dp"
        android:layout_height="60dp"
        android:src="@drawable/logokembali"
        android:id="@+id/imageView60"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:onClick="kembali" />


    <ImageView
        android:layout_width="180dp"
        android:layout_height="60dp"
        android:src="@drawable/logomenuutama"
        android:layout_alignBottom="@+id/imageView60"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:onClick="menuutama" />


</RelativeLayout>

<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:orientation="vertical"
    android:background="@drawable/backgroundputih"
    tools:context="net.fanidyacinthya.penulisanilmiah.kampusf5">

    <ImageView
        android:layout_width="300dp"
        android:layout_height="250dp"
        android:src="@drawable/logof5"
        android:id="@+id/imageView69"
        android:layout_marginBottom="418dp"
        android:layout_above="@+id/imageView95"
        android:layout_centerHorizontal="true" />

    <ImageView
        android:layout_width="400dp"
        android:layout_height="230dp"
        android:id="@+id/imageView"
        android:src="@drawable/kampus_f5a"
        android:layout_marginTop="66dp"
        android:layout_alignTop="@+id/imageView69"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />


    <ImageView
        android:layout_width="190dp"
        android:layout_height="60dp"
        android:src="@drawable/iconinformasi"
        android:id="@+id/imageView70"
        android:layout_below="@+id/imageView"
        android:layout_centerHorizontal="true"
        android:onClick="informasi" />

    <ImageView
        android:layout_width="190dp"
        android:layout_height="60dp"
        android:src="@drawable/iconlokasi"
        android:layout_below="@+id/imageView70"
        android:layout_alignLeft="@+id/imageView70"
        android:layout_alignStart="@+id/imageView70"
        android:onClick="lokasi" />

    <ImageView
        android:layout_width="155dp"
        android:layout_height="70dp"
        android:src="@drawable/logokembali"
        android:layout_alignTop="@+id/imageView71"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:onClick="kembali"
        android:id="@+id/imageView95" />


    <ImageView
        android:layout_width="180dp"
        android:layout_height="60dp"
        android:src="@drawable/logomenuutama"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:id="@+id/imageView71"
        android:onClick="menuutama" />
</RelativeLayout>
<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:orientation="vertical"
    android:background="@drawable/backgroundputih"
    tools:context="net.fanidyacinthya.penulisanilmiah.kampusd">


    <ImageView
        android:layout_width="300dp"
        android:layout_height="250dp"
        android:src="@drawable/logod"
        android:layout_marginBottom="412dp"
        android:id="@+id/imageView42"
        android:layout_above="@+id/imageView45"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignRight="@+id/imageView43"
        android:layout_alignEnd="@+id/imageView43" />

    <ImageView
        android:layout_width="400dp"
        android:layout_height="250dp"
        android:src="@drawable/kampus_d1"
        android:layout_marginTop="75dp"
        android:id="@+id/imageView43"
        android:layout_alignTop="@+id/imageView42"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />


    <ImageView
        android:layout_width="190dp"
        android:layout_height="70dp"
        android:src="@drawable/iconinformasi"
        android:id="@+id/imageView44"
        android:layout_below="@+id/imageView43"
        android:layout_centerHorizontal="true"
        android:onClick="informasi" />

    <ImageView
        android:layout_width="190dp"
        android:layout_height="90dp"
        android:src="@drawable/iconlokasi"
        android:layout_alignTop="@+id/imageView44"
        android:layout_alignLeft="@+id/imageView44"
        android:layout_alignStart="@+id/imageView44"
        android:layout_marginTop="44dp"
        android:id="@+id/imageView47"
        android:onClick="lokasi" />

    <ImageView
        android:layout_width="155dp"
        android:layout_height="60dp"
        android:src="@drawable/logokembali"
        android:id="@+id/imageView45"
        android:onClick="kembali"
        android:layout_alignTop="@+id/imageView46"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />


    <ImageView
        android:layout_width="180dp"
        android:layout_height="50dp"
        android:src="@drawable/logomenuutama"
        android:id="@+id/imageView46"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:onClick="menuutama" />

</RelativeLayout>

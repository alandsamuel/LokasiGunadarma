<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:orientation="vertical"
    android:background="@drawable/backgroundputih"
    tools:context="net.fanidyacinthya.penulisanilmiah.menumasuk">

    <ImageView
        android:layout_width="150dp"
        android:layout_height="200dp"
        android:src="@drawable/daftarkampus2"
        android:id="@+id/imageView2"
        android:layout_alignParentTop="true"
        android:layout_alignRight="@+id/imageView3"
        android:layout_alignEnd="@+id/imageView3"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <ImageView
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:src="@drawable/logodaftarkampus"
        android:id="@+id/imageView3"
        android:onClick="dftrkampus"
        android:layout_alignTop="@+id/imageView5"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_toLeftOf="@+id/imageView4"
        android:layout_toStartOf="@+id/imageView4" />

    <ImageView
        android:layout_width="120dp"
        android:layout_height="150dp"
        android:src="@drawable/menubantuan"
        android:id="@+id/imageView4"
        android:layout_alignBottom="@+id/imageView2"
        android:layout_alignLeft="@+id/imageView5"
        android:layout_alignStart="@+id/imageView5"
        android:layout_alignTop="@+id/imageView2"
        android:layout_alignRight="@+id/imageView9"
        android:layout_alignEnd="@+id/imageView9" />

    <ImageView
        android:layout_width="160dp"
        android:layout_height="150dp"
        android:src="@drawable/logobantuan"
        android:id="@+id/imageView5"
        android:onClick="bantuan"
        android:layout_above="@+id/imageView8"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <ImageView
        android:layout_width="150dp"
        android:layout_height="130dp"
        android:src="@drawable/menutentang"
        android:id="@+id/imageView6"
        android:layout_marginTop="72dp"
        android:layout_below="@+id/imageView2"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignRight="@+id/imageView3"
        android:layout_alignEnd="@+id/imageView3" />

    <ImageView
        android:layout_width="170dp"
        android:layout_height="70dp"
        android:src="@drawable/logotentang"
        android:id="@+id/imageView7"
        android:onClick="tentang"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <ImageView
        android:layout_width="160dp"
        android:layout_height="140dp"
        android:src="@drawable/menukeluar"
        android:id="@+id/imageView8"
        android:layout_alignTop="@+id/imageView6"
        android:layout_alignLeft="@+id/imageView5"
        android:layout_alignStart="@+id/imageView5" />

    <ImageView
        android:layout_width="200dp"
        android:layout_height="100dp"
        android:src="@drawable/logokeluar"
        android:id="@+id/imageView9"
        android:onClick="keluar"
        android:layout_below="@+id/imageView8"
        android:layout_alignBottom="@+id/imageView7"
        android:layout_toRightOf="@+id/imageView7"
        android:layout_toEndOf="@+id/imageView7" />

</RelativeLayout>
<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:orientation="vertical"
    android:background="@drawable/backgroundputih"
    tools:context="net.fanidyacinthya.penulisanilmiah.kampusg">


    <ImageView
        android:layout_width="300dp"
        android:layout_height="200dp"
        android:src="@drawable/logog"
        android:id="@+id/imageView73"
        android:layout_marginBottom="436dp"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true" />

    <ImageView
        android:layout_width="400dp"
        android:layout_height="250dp"
        android:src="@drawable/kampus_g1"
        android:layout_alignTop="@+id/imageView73"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="90dp"
        android:id="@+id/imageView74" />

    <ImageView
        android:layout_width="190dp"
        android:layout_height="60dp"
        android:src="@drawable/iconinformasi"
        android:layout_below="@+id/imageView74"
        android:layout_centerHorizontal="true"
        android:id="@+id/imageView75"
        android:onClick="informasi" />

    <ImageView
        android:layout_width="180dp"
        android:layout_height="60dp"
        android:src="@drawable/iconlokasi"
        android:layout_alignLeft="@+id/imageView75"
        android:layout_alignStart="@+id/imageView75"
        android:layout_alignRight="@+id/imageView75"
        android:layout_alignEnd="@+id/imageView75"
        android:layout_below="@+id/imageView75"
        android:onClick="lokasi" />

    <ImageView
        android:layout_width="155dp"
        android:layout_height="60dp"
        android:src="@drawable/logokembali"
        android:id="@+id/imageView76"
        android:layout_alignTop="@+id/imageView77"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:onClick="kembali" />

    <ImageView
        android:layout_width="180dp"
        android:layout_height="50dp"
        android:src="@drawable/logomenuutama"
        android:layout_alignParentBottom="true"
        android:layout_alignRight="@+id/imageView74"
        android:layout_alignEnd="@+id/imageView74"
        android:id="@+id/imageView77"
        android:onClick="menuutama" />
</RelativeLayout>